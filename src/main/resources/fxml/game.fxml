<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="gameZone" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #0347CF;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.GameController">
    <children>
        <Canvas fx:id="playGroundCanvas" height="600.0" width="800.0" />
      <Button fx:id="btnBack" layoutX="695.0" layoutY="538.0" mnemonicParsing="false" onAction="#backToMenu" text="Menu" visible="false" />
      <Rectangle fx:id="obstacle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="59.0" stroke="TRANSPARENT" strokeType="INSIDE" width="60.0" />
      <Rectangle fx:id="carRectangle" arcHeight="5.0" arcWidth="5.0" fill="#5bff1f" height="128.0" stroke="TRANSPARENT" strokeType="INSIDE" width="64.0" />
      <Label fx:id="gameOverLable" contentDisplay="CENTER" layoutX="264.0" layoutY="150.0" prefHeight="96.0" prefWidth="272.0" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Label fx:id="scoreLBL" layoutX="546.0" layoutY="14.0" prefHeight="45.0" prefWidth="229.0" textFill="#d00808">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
